! -1 is default
Xft.dpi: 144
Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.rgba: rgb

! -----------------------------------------------------------------------------
! Colorscheme: Gruvbox Material Dark
! Description: Modern groove colorscheme with softer contrast
! Author: sainnhe
! Source: https://github.com/sainnhe/gruvbox-material
! -----------------------------------------------------------------------------

! medium contrast: *background: #282828
! softer contrast: *background: #32302f
*background: #1d2021
*foreground: #d4be98

! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #ea6962
*color9:  #ef938e
! DarkGreen + Green
*color2:  #a9b665
*color10: #bbc585
! DarkYellow + Yellow
*color3:  #d8a657
*color11: #e1bb7e
! DarkBlue + Blue
*color4:  #7daea3
*color12: #9dc2ba
! DarkMagenta + Magenta
*color5:  #d3869b
*color13: #e1acbb
! DarkCyan + Cyan
*color6:  #89b482
*color14: #a7c7a2
! LightGrey + White
*color7:  #d4be98
*color15: #e2d3ba

