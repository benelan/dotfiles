{
  "name": "@benelan/vscode-config",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "global",
          "all"
        ],
        "path": "snippets/global.json"
      },
      {
        "language": [
          "html"
        ],
        "path": "snippets/html.json"
      },
      {
        "language": [
          "javascript"
        ],
        "path": "snippets/javascript.json"
      },
      {
        "language": [
          "typescriptreact"
        ],
        "path": "snippets/stencil.json"
      }
    ]
  }
}
