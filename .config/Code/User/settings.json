{
  "workbench.colorTheme": "Gruvbox Dark Soft",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.wordWrap": "off",
  "editor.wordWrap": "on",
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.autoClosingDelete": "never",
  "editor.suggestSelection": "first",
  "editor.linkedEditing": true,
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "files.associations": {
    "hjson": "json",
    "jsonc": "json"
  },
  "editor.fontLigatures": true,
  "editor.insertSpaces": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "editor.fontFamily": "'JetBrainsMono Nerd Font', 'SauceCodePro Nerd Font', 'Droid Sans Mono', 'monospace', monospace",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": true,
  "explorer.compactFolders": false,
  "explorer.sortOrder": "type",
  "extensions.autoUpdate": false,
  "extensions.autoCheckUpdates": true,
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "files.eol": "\n",
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.hotExit": "onExitAndWindowClose",
  "files.autoSave": "off",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.allowForcePush": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.surveys.enabled": false,
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode",
  },
  "terminal.external.linuxExec": "kitty",
  "terminal.external.windowsExec": "C:\\Program Files\\Git\\bin\\bash.exe",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "liveServer.settings.donotShowInfoMsg": true,
  "search.smartCase": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/build": true,
    "**/dist": true,
    "*.min.js": true
  },
  "cSpell.userWords": [
    "arcgis",
    "benelan",
    "browserslist",
    "lazyload",
    "outfile",
    "JSAPI",
    "transpilation",
    "transpiled",
    "transpiling",
    "vsintellicode",
    "easymotion",
    "incsearch",
    "hlsearch",
    "autofetch",
    "hjson",
    "gruvbox",
    "esbenp",
  ],
  "vim.easymotion": false,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-j>"],
      "after": ["<leader>", "<leader>", "j"]
    },
    {
      "before": ["<C-k>"],
      "after": ["<leader>", "<leader>", "k"]
    },
    {
      "before": ["<C-l>"],
      "commands": [":nohl"]
    },
    {
      "before": ["leader", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["Z", "Z"],
      "commands": [":wq"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["editor.action.commentLine"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "/"],
      "commands": ["editor.action.commentLine"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },
    {
      "before": ["<C-a>"],
      "commands": ["editor.action.selectAll"]
    },
    {
      "before": ["<C-c>"],
      "commands": ["editor.action.clipboardCopyAction"]
    },
    {
      "before": ["<C-v>"],
      "commands": ["editor.action.clipboardPasteAction"]
    },
    {
      "before": ["<C-x>"],
      "commands": ["editor.action.clipboardCutAction"]
    }
  ]
}
