{
  "workbench.colorTheme": "Gruvbox Dark Soft",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.wordWrap": "off",
  "editor.wordWrap": "on",
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.autoClosingDelete": "never",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingOvertype": "never",
  "editor.autoClosingQuotes": "never",
  "editor.suggestSelection": "first",
  "editor.linkedEditing": true,
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "files.associations": {
    "hjson": "json",
    "jsonc": "json"
  },
  "editor.fontLigatures": true,
  "editor.insertSpaces": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "editor.fontFamily": "'Iosevka Nerd Font', 'JetBrainsMono Nerd Font', 'SauceCodePro Nerd Font', 'Droid Sans Mono', 'monospace', monospace",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": true,
  "explorer.compactFolders": false,
  "explorer.sortOrder": "type",
  "extensions.autoUpdate": false,
  "extensions.autoCheckUpdates": true,
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "files.eol": "\n",
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.hotExit": "onExitAndWindowClose",
  "files.autoSave": "off",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.allowForcePush": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.surveys.enabled": false,
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "Lua.diagnostics.globals": ["vim"],
  "Lua.runtime.version": "LuaJIT",
  "Lua.telemetry.enable": false,
  "Lua.completion.callSnippet": "Both",
  "Lua.completion.keywordSnippet": "Both",
  "terminal.external.linuxExec": "kitty",
  "terminal.external.windowsExec": "C:\\Program Files\\Git\\bin\\bash.exe",
  // "vscode-neovim.neovimExecutablePaths.linux": "~/.dotfiles/vendor/neovim/build/bin/nvim",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "liveServer.settings.donotShowInfoMsg": true,
  "search.smartCase": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/build": true,
    "**/dist": true,
    "*.min.js": true
  },
  "cSpell.userWords": [
    "dotfiles",
    "neovim",
    "arcgis",
    "benelan",
    "browserslist",
    "lazyload",
    "outfile",
    "JSAPI",
    "transpilation",
    "transpiled",
    "transpiling",
    "vsintellicode",
    "easymotion",
    "incsearch",
    "hlsearch",
    "autofetch",
    "hjson",
    "gruvbox",
    "esbenp"
  ],
  "vim.easymotion": false,
  "vim.enableNeovim": true,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.showMarksInGutter": true,
  "vim.smartRelativeLine": true,
  "vim.surround": true,
  "vim.targets.bracketObjects.enable": true,
  "vim.targets.enable": true,
  "vim.targets.smartQuotes.enable": true,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "<C-l>"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.explorer.fileView.focus"]
    },
    {
      "before": ["leader", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["Z", "Z"],
      "commands": [":wq"]
    },
    {
      "before": ["<leader>", "/"],
      "commands": ["editor.action.commentLine"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "/"],
      "commands": ["editor.action.commentLine"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["<C-V>"],
      "commands": ["editor.action.clipboardPasteAction"]
    }
  ],
  "window.zoomLevel": 1
}
