{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT"
  },
  "diagnostics": {
    "libraryFiles": "Disable",
    "globals": ["vim"]
  },
  "workspace.checkThirdParty": false,
  "workspace.library": [
    "./lua",
    "$VIMRUNTIME",
    "${3rd}/luv/library",
    "${3rd}/luassert/library",
    "$XDG_DATA_HOME/nvim/lazy/gitsigns.nvim/lua",
    "$XDG_DATA_HOME/nvim/lazy/lazy.nvim/lua",
    "$XDG_DATA_HOME/nvim/lazy/blink.cmp/lua",
    "$XDG_DATA_HOME/nvim/lazy/copilot.lua/lua",
    "$XDG_DATA_HOME/nvim/lazy/CopilotChat.nvim/lua",
    "$XDG_DATA_HOME/nvim/lazy/flash.nvim/lua",
    "$XDG_DATA_HOME/nvim/lazy/mason-lspconfig.nvim/lua",
    "$XDG_DATA_HOME/nvim/lazy/mason.nvim/lua",
    "$XDG_DATA_HOME/nvim/lazy/neotest/lua",
    "$XDG_DATA_HOME/nvim/lazy/nvim-dap-ui/lua",
    "$XDG_DATA_HOME/nvim/lazy/nvim-dap/lua",
    "$XDG_DATA_HOME/nvim/lazy/nvim-lspconfig/lua",
    "$XDG_DATA_HOME/nvim/lazy/nvim-nio/lua",
    "$XDG_DATA_HOME/nvim/lazy/nvim-treesitter/lua",
    "$XDG_DATA_HOME/nvim/lazy/octo.nvim/lua",
    "$XDG_DATA_HOME/nvim/lazy/plenary.nvim/lua",
    "$XDG_DATA_HOME/nvim/lazy/snacks.nvim/lua",
    "$XDG_DATA_HOME/nvim/lazy/telescope.nvim/lua",
    "$XDG_DATA_HOME/nvim/lazy/debugprint.nvim/lua",
    "$XDG_DATA_HOME/nvim/lazy/harpoon/lua",
    "$XDG_DATA_HOME/nvim/lazy/nvim-rulebook/lua",
    "$XDG_DATA_HOME/nvim/lazy/none-ls.nvim/lua"
  ]
}
