# https://editorconfig.org/

root = true

[*]
    charset = utf-8
    end_of_line = lf
    trim_trailing_whitespace = true
    indent_style = space
    quote_type = double
    spaces_around_operators = true
    spaces_around_brackets = false

[*.{js,jsx,ts,tsx,vue,svelte}]
    spaces_around_brackets = true

[Makefile]
    indent_style = tab
